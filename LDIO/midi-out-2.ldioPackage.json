{
    "$schema": "http://www.lavishsoft.com/schema/ldioPackage.json",
    "patches": [     
        [
            {
                "type":"device-out",
                "name":"synth",
                "device": "Microsoft GS Wavetable Synth"
            },
            {
                "type":"log"
            }
        ],
        [
            {
                "name":"timer-1",
                "type":"timer",
                "command":["pulse"],
                "interval": 0.5            
